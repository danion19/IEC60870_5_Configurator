	RETURN;
ELSE
	wMeasureOutput1 := wMeasure1;
wMeasureOutput2 := wMeasure2;
wMeasureOutput3 := wMeasure3;
wMeasureOutput4 := wMeasure4;
wMeasureOutput5 := wMeasure5;
wMeasureOutput6 := wMeasure6;
